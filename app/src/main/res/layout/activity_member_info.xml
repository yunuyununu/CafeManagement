<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MemberInfo">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnUpdate"
        android:layout_width="97dp"
        android:layout_height="57dp"
        android:background="@drawable/round_border"
        android:fontFamily="@font/nanumbarungothic"
        android:text="수정"
        android:textColor="#232323"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.216"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.727" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnDelete"
        android:layout_width="97dp"
        android:layout_height="57dp"
        android:background="@drawable/round_border"
        android:fontFamily="@font/nanumbarungothic"
        android:text="삭제"
        android:textColor="#232323"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.783"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.727" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="352dp"
        android:layout_height="210dp"
        android:background="@drawable/one"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.389">

        <LinearLayout
            android:layout_width="272dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/two"
            android:orientation="vertical">

            <TextView
                android:id="@+id/text10"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:background="@drawable/two"
                android:fontFamily="@font/nanumbarungothic"
                android:gravity="center"
                android:text=" 적립번호"
                android:textAlignment="viewStart"
                android:textColor="#232323"
                android:textSize="19dp" />

            <TextView
                android:id="@+id/text11"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:background="@color/beige3"
                android:fontFamily="@font/nanumbarungothic"
                android:gravity="center"
                android:text=" 전화번호"
                android:textAlignment="viewStart"
                android:textColor="#232323"
                android:textSize="19dp" />

            <TextView
                android:id="@+id/text12"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:background="@color/beige3"
                android:fontFamily="@font/nanumbarungothic"
                android:gravity="center"
                android:text=" 가입일자"
                android:textAlignment="viewStart"
                android:textColor="#232323"
                android:textSize="19dp" />

            <TextView
                android:id="@+id/text13"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:background="@color/beige3"
                android:fontFamily="@font/nanumbarungothic"
                android:gravity="center"
                android:text=" 주문횟수"
                android:textAlignment="viewStart"
                android:textColor="#232323"
                android:textSize="19dp" />

            <TextView
                android:id="@+id/text14"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:background="@drawable/two"
                android:fontFamily="@font/nanumbarungothic"
                android:gravity="center"
                android:text=" 쿠폰"
                android:textAlignment="viewStart"
                android:textColor="#232323"
                android:textSize="19dp" />

            <TextView
                android:id="@+id/text15"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:background="@drawable/two"
                android:fontFamily="@font/nanumbarungothic"
                android:gravity="center"
                android:text=" 스탬프"
                android:textAlignment="viewStart"
                android:textColor="#232323"
                android:textSize="19dp" />

            <TextView
                android:id="@+id/text16"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:background="@drawable/two"
                android:fontFamily="@font/nanumbarungothic"
                android:gravity="center"
                android:text=" 마지막 주문일자"
                android:textAlignment="viewStart"
                android:textColor="#232323"
                android:textSize="19dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="316dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <EditText
                android:id="@+id/txtId"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:background="@null"
                android:ems="10"
                android:fontFamily="@font/nanumbarungothic"
                android:gravity="center"
                android:inputType="number"
                android:textAlignment="viewStart"
                android:textColor="#3E3E3E"
                android:textSize="20sp" />

            <EditText
                android:id="@+id/txtPhone"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:background="@null"
                android:ems="10"
                android:fontFamily="@font/nanumbarungothic"
                android:gravity="center"
                android:inputType="phone"
                android:maxLength="13"
                android:textAlignment="viewStart"
                android:textColor="#3E3E3E"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/txtSdate"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:fontFamily="@font/nanumbarungothic"
                android:gravity="center"
                android:text="TextView"
                android:textAlignment="viewStart"
                android:textColor="@color/black"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/txtOrderc"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:fontFamily="@font/nanumbarungothic"
                android:gravity="center"
                android:text="TextView"
                android:textAlignment="viewStart"
                android:textColor="@color/black"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/txtCoupon"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:fontFamily="@font/nanumbarungothic"
                android:gravity="center"
                android:text="TextView"
                android:textAlignment="viewStart"
                android:textColor="@color/black"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/txtStamp"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:fontFamily="@font/nanumbarungothic"
                android:gravity="center"
                android:text="TextView"
                android:textAlignment="viewStart"
                android:textColor="@color/black"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/txtOdate"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:fontFamily="@font/nanumbarungothic"
                android:gravity="center"
                android:text="TextView"
                android:textAlignment="viewStart"
                android:textColor="@color/black"
                android:textSize="20sp" />
        </LinearLayout>

    </LinearLayout>

    <TextView
        android:id="@+id/textView"
        android:layout_width="148dp"
        android:layout_height="51dp"
        android:background="@drawable/three"
        android:fontFamily="@font/nanumbarungothic"
        android:gravity="center"
        android:text="회원정보"
        android:textAlignment="center"
        android:textColor="#232323"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.119" />

</androidx.constraintlayout.widget.ConstraintLayout>